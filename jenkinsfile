Pipeline{
         agent any
	 stages{
		stage("Fetching the git repository")
		  {
 	           checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'githubcred', url: 'https://github.com/prathmeshrajmane/-Ansible-CI-CD-pipeline.git']]])
		  }
	        }
	  	stage("Execute Ansible playbook")
		 {
		   sh 'ansible-playbook setup.yml
		 }
	}
		 
