peline{
	agent any
	stages{
		stage('SCM Checkout') {
			steps {
               checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/prathmeshrajmane/-Ansible-CI-CD-pipeline.git']]])                 }
			}
                stage("Execute Ansible playbook")
                 {
                     steps{
                         
                   sh 'ansible-playbook setup.yml'
                     
                     }
                 }
                 stage("Testing")
                 {
                     steps{
                    sh 'sh ip.sh'
                     }
                 }
        }
}
